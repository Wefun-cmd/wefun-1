#include<stdio.h>
int main()
{
        int i = 0;
        int n = 0;
        int ret = 1;
        int sum = 0;
        for (n = 1; n <= 3; n ++ )
        {
            for (i = 1; i <= n; i++)
            {
                ret = ret * i;
            }
            sum = sum + ret;
        }
        printf("%d", sum);
        return 0;
    }
